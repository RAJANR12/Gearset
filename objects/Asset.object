    <validationRules>
        <fullName>Cannot_populate_on_Parent_asset_lookup</fullName>
        <active>true</active>
        <description>This error will be displayed if the user attempts in input a value on the Order Quantity field on the Parent asset</description>
        <errorConditionFormula>ISCHANGED(PT_Order_Quantity__c) &amp;&amp; ISBLANK(PT_Case__c)</errorConditionFormula>
        <errorDisplayField>PT_Order_Quantity__c</errorDisplayField>
        <errorMessage>Order Quantity can only be updated on the Child Asset level</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Conformity_not_allowed_on_Prod_Prog_Type</fullName>
        <active>true</active>
        <errorConditionFormula>BEGINS(PT_Program__r.PT_Program_Type__c, &quot;Production&quot;) &amp;&amp; ISPICKVAL(PT_Conformity_Overwritten__c, &quot;Yes&quot;)</errorConditionFormula>
        <errorMessage>Conformity can not be true or overwritten when Program Type is Production</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Disallow_Asset_updates</fullName>
        <active>false</active>
        <description>Asset updates not allowed when related Order Product is NOT in a Status of &quot;Booked&quot;, &quot;Entered&quot; or &quot;Awaiting shipping&quot;</description>
        <errorConditionFormula>/*OR(
SBQQ__OrderProduct__r.PT_Ext_Order_Line_Status__c =  &quot;Booked&quot;,*/
		/*SBQQ__OrderProduct__r.PT_Ext_Order_Line_Status__c = &quot;Entered&quot;,*/
			/*	&quot;&quot;SBQQ__OrderProduct__r.PT_Ext_Order_Line_Status__c =	&quot;Awaiting shipping&quot;*/
				
		AND(		
						 ISBLANK(PT_Ship_to_Location__c ))</errorConditionFormula>
        <errorMessage>Updates are no longer allowed for this Part as it has been closed in Oracle</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_Qty_Cannot_be_less_than_0</fullName>
        <active>true</active>
        <errorConditionFormula>PT_Order_Quantity__c &lt;   0</errorConditionFormula>
        <errorDisplayField>PT_Order_Quantity__c</errorDisplayField>
        <errorMessage>Order Quantity cannot be less than 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Order_Qty_must_be_less_Available_Qt</fullName>
        <active>false</active>
        <errorConditionFormula>PT_Order_Quantity__c &gt; Parent.PT_Available_Quantity__c</errorConditionFormula>
        <errorDisplayField>PT_Order_Quantity__c</errorDisplayField>
        <errorMessage>Inputted Order Quantity results in a total that is greater than the remaining Available Quantity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PT_Prevent_Asset_update_OrderClosed_Case</fullName>
        <active>true</active>
        <description>Assets cannot be updated when the associated Customer PO Case is in a Status of &quot;Order Closed&quot;</description>
        <errorConditionFormula>AND(
ISPICKVAL(PT_Case__r.Status, &quot;Order Closed&quot;),
ISPICKVAL( PT_Case__r.PO_Case_Type__c, &quot;IFEC&quot;),
NOT( ISBLANK( 				 PT_Case__c )),
NOT (ISBLANK(ParentId)))</errorConditionFormula>
        <errorDisplayField>AccountId</errorDisplayField>
        <errorMessage>This Asset cannot be updated because it is related to a Customer PO Case that is in a Status of &quot;Order Closed&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Update_Order_Product_is_Closed</fullName>
        <active>true</active>
        <description>Prevent Asset Update if Related Order Product is Closed (Oracle Status)</description>
        <errorConditionFormula>OR(UPPER(PT_Oracle_Line_Status__c) = &quot;CANCELLED&quot;, UPPER(PT_Oracle_Line_Status__c) = &quot;CLOSED&quot;, UPPER(PT_Oracle_Line_Status__c) = &quot;FULFILLED&quot;, UPPER(PT_Oracle_Line_Status__c) = &quot;PICKED&quot;, UPPER(PT_Oracle_Line_Status__c) = &quot;PICKED_PARTIAL&quot;, UPPER(PT_Oracle_Line_Status__c) = &quot;SHIPPED&quot;, UPPER(PT_Oracle_Line_Status__c) = &quot;AWAITING_EXPORT_SCREENING&quot;, UPPER(PT_Oracle_Line_Status__c) = &quot;RELEASED_TO_WAREHOUSE&quot;, UPPER(PT_Oracle_Line_Status__c) = &quot;PENDING_APPROVAL&quot;, UPPER(PT_Oracle_Line_Status__c) = &quot;APPROVED&quot;, UPPER(PT_Oracle_Line_Status__c) = &quot;INVOICE_HOLD&quot;)
 
&amp;&amp;
 
OR(ISCHANGED(PT_Order_Quantity__c), ISCHANGED(PT_Warehouse__c), ISCHANGED(PT_Line_Type__c), ISCHANGED(PT_Customer_PO_Line__c), ISCHANGED(PT_Customer_PO_Request_Date__c), ISCHANGED(PT_Reference_PO__c), ISCHANGED(PT_Reference_PO_Request_Date__c), ISCHANGED(PT_LF_Software_Supply_Date__c), ISCHANGED(PT_Ship_to_Location__c), ISCHANGED(PT_Additional_AR_Invoice_Notes__c), ISCHANGED(PT_Comments__c))</errorConditionFormula>
        <errorMessage>Updates are no longer allowed for this Part as it has been closed in Oracle</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Qty_cannot_be_greater_than_Config_Qty</fullName>
        <active>false</active>
        <errorConditionFormula>AND($Profile.Name &lt;&gt; &quot;System Administrator&quot; ,
OR ( AND (NOT(ISBLANK(PT_Pending_Quantity__c)), NOT(ISBLANK(PT_Pending_Configured_Quantity__c)), PT_Pending_Quantity__c  &gt;  PT_Pending_Configured_Quantity__c),
     AND (NOT(ISBLANK(PT_Pending_Quantity__c)), ISBLANK(PT_Pending_Configured_Quantity__c)) )
     )</errorConditionFormula>
        <errorMessage>Quantity cannot be greater than Configured Quantity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Total_Order_Qty_cannot_exceed_Quantity</fullName>
        <active>true</active>
        <errorConditionFormula>/*ISCHANGED(PT_Order_Quantity__c) &amp;&amp;
Parent.PT_Total_Order_Quantity__c &gt; Parent.Quantity

!ISBLANK(ParentId) &amp;&amp; ISCHANGED(PT_Order_Quantity__c) &amp;&amp; IF(ISBLANK(Parent.PT_Total_Order_Quantity__c), 
(PT_Available_Quantity__c - PT_Order_Quantity__c) &lt; 0,
PT_Order_Quantity__c &gt; PT_Available_Quantity__c
)*/

!ISBLANK(ParentId) &amp;&amp; ISCHANGED(PT_Order_Quantity__c) &amp;&amp; IF(ISBLANK(Parent.PT_Total_Order_Quantity__c), 
(PT_Available_Quantity__c - PT_Order_Quantity__c) &lt; 0,
IF(ISBLANK(PRIORVALUE(PT_Order_Quantity__c)), 
(Parent.Quantity - (Parent.PT_Total_Order_Quantity__c + PT_Order_Quantity__c )) &lt; 0,
(Parent.Quantity - ((Parent.PT_Total_Order_Quantity__c - PRIORVALUE(PT_Order_Quantity__c)) + PT_Order_Quantity__c )) &lt; 0
))</errorConditionFormula>
        <errorDisplayField>PT_Order_Quantity__c</errorDisplayField>
        <errorMessage>Order Quantity cannot be greater than Available Quantity, please reduce Order Quantity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Total_Order_qty_not_more_than_available</fullName>
        <active>false</active>
        <errorConditionFormula>Parent.PT_Total_Order_Quantity__c  &gt;  Parent.PT_Available_Quantity__c</errorConditionFormula>
        <errorMessage>Total Order Quantity cannot be greater than the remaining available Quantity</errorMessage>
    </validationRules>
