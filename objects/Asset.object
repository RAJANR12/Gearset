    <fields>
        <fullName>PT_CCC__c</fullName>
        <externalId>false</externalId>
        <formula>IF( ISBLANK(PT_Install_Location__r.ShipSet__c), 

  IF ( PT_Install_Location__r.Solution_Group__r.PT_is_IFC__c &amp;&amp; NOT(BEGINS(PT_Program__r.Program_Number__c, &quot;E&quot;)), 

	&quot;E&quot; &amp; RIGHT(PT_Program__r.PT_Program_Number_Alt__c, 7),

	PT_Program__r.Program_Number__c ),

	IF ( PT_Install_Location__r.Solution_Group__r.PT_is_IFC__c &amp;&amp; NOT(BEGINS(PT_Program__r.Program_Number__c, &quot;E&quot;)), 

	&quot;E&quot; &amp; RIGHT(PT_Program__r.PT_Program_Number_Alt__c, 7),

    PT_Program__r.Program_Number__c )

 )

&amp; 

IF( ISBLANK(PT_Install_Location__r.ShipSet__c), 

   TEXT(PT_Install_Location__r.Part_List_Opportunity__r.PT_PPL_Type__c),

     PT_Install_Location__r.ShipSet__r.PT_Line_Number__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Cost Collection Code Formula</inlineHelpText>
        <label>Cost Collection Code (CCC)</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
