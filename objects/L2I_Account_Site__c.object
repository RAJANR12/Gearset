IF( ISBLANK(State__c) , &quot;&quot;, State__c + &quot; &quot;) + 
IF( ISBLANK(Postal_Code__c) , &quot;&quot;, Postal_Code__c) +
IF( ISBLANK(County__c) , &quot;&quot;, BR() + County__c) +
