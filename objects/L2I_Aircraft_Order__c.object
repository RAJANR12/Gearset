    <validationRules>
        <fullName>Activation_Deactivation_Date_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>( $Profile.Name &lt;&gt; &quot;System Administrator&quot; &amp;&amp; 
	 $Profile.Name &lt;&gt; &quot;PAC System Administrator&quot; &amp;&amp; 
	 $Profile.Name &lt;&gt; &quot;PAC Integration&quot; )
&amp;&amp; 
( ( ISCHANGED(Activation_Date__c) &amp;&amp;
		  (TODAY() - DATEVALUE(Activation_Date__c) &gt;7)
			)
  || 
	 ( ISCHANGED(Deactivation_Date__c) &amp;&amp;
		  (TODAY() - DATEVALUE(Deactivation_Date__c) &gt;7)
		)
)</errorConditionFormula>
        <errorMessage>Activation Date or Deactivation Date cannot be set older then 7 days.</errorMessage>
    </validationRules>
