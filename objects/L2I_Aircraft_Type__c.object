        <externalId>false</externalId>
        <formula>IF(TEXT(L2I_Type_Level_ID__c) = &#39;1&#39;, &quot;Model&quot;,
IF(TEXT(L2I_Type_Level_ID__c) = &#39;2&#39;, &quot;Master Series&quot;,
IF(TEXT(L2I_Type_Level_ID__c) = &#39;3&#39;, &quot;Series&quot;, &quot;Series&quot;)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
