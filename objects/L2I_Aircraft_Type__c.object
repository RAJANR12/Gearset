    <validationRules>
        <fullName>L2I_Aircraft_Type_Name_Unique</fullName>
        <active>true</active>
        <description>Aircraft Type Name must be unique</description>
        <errorConditionFormula>and( 
Name = VLOOKUP 
($ObjectType.L2I_Aircraft_Type__c.Fields.Name , $ObjectType.L2I_Aircraft_Type__c.Fields.Name, Name ), 

not( 
Id=VLOOKUP 
($ObjectType.L2I_Aircraft_Type__c.Fields.Id , $ObjectType.L2I_Aircraft_Type__c.Fields.Name, Name )))</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>The name of your Aircraft Type must be unique</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Master_Series_Required</fullName>
        <active>true</active>
        <description>Master Series is required if Type Level ID = 2</description>
        <errorConditionFormula>AND(
  L2I_Master_Series__c = &quot;&quot;,
   TEXT(L2I_Type_Level_ID__c) = &quot;2&quot;)</errorConditionFormula>
        <errorDisplayField>L2I_Master_Series__c</errorDisplayField>
        <errorMessage>Master Series cannot be blank if Type Level ID = 2</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Series_Required</fullName>
        <active>true</active>
        <description>Series is required if Type Level ID = 3</description>
        <errorConditionFormula>AND( 
L2I_Series__c = &quot;&quot;, 
TEXT(L2I_Type_Level_ID__c) = &quot;3&quot;)</errorConditionFormula>
        <errorDisplayField>L2I_Series__c</errorDisplayField>
        <errorMessage>Series cannot be blank if Type Level ID = 3</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Type_Level_ID_1_Master_Series_and_Series</fullName>
        <active>true</active>
        <description>Validation
If Type Level = 1 Then Series and Master Series Must be Blank</description>
        <errorConditionFormula>AND(
(L2I_Type_Level_ID__c = 1),
OR(
NOT(ISBLANK(L2I_Master_Series__c)),
NOT(ISBLANK(L2I_Series__c))))</errorConditionFormula>
        <errorMessage>If Type Level ID = 1 Then Series and Master Series Must be Blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Type_Level_ID_2_Series</fullName>
        <active>true</active>
        <description>If Type Level ID = 2 Series must be blank</description>
        <errorConditionFormula>AND(
(L2I_Type_Level_ID__c = 2),
AND(NOT(ISBLANK(L2I_Series__c))
))</errorConditionFormula>
        <errorDisplayField>L2I_Series__c</errorDisplayField>
        <errorMessage>If Type Level ID = 2 Then Series Must be Blank</errorMessage>
    </validationRules>
