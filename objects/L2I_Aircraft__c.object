    <validationRules>
        <fullName>Approved_Aircraft_Lockdown_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT($Permission.L2I_Aircraft_Data_Steward_Edit),
(L2I_Approved__c = TRUE),
OR(
ISCHANGED(L2I_Aircraft_Status__c),
ISCHANGED(Name),
ISCHANGED(L2I_Owner__c), 
ISCHANGED(Aircraft_Type__c), 
ISCHANGED(L2I_Aircraft_Status__c),
ISCHANGED(L2I_GPIN__c),
ISCHANGED(L2I_Preliminary_GPIN__c),
ISCHANGED(L2I_Serial_Number__c),
ISCHANGED(L2I_Block_Number__c),
ISCHANGED(L2I_PAC_Aircraft_Master_Database_ID__c), 
ISCHANGED(L2I_Registration_Number__c), 
ISCHANGED(L2I_Fleet_Number__c),
ISCHANGED(L2I_Line_Number__c),
ISCHANGED(L2I_Aircraft_Delivery_Date__c),
ISCHANGED(OwnerId),
ISCHANGED(L2I_Operator__c)
))</errorConditionFormula>
        <errorMessage>Aircraft records that have been Approved can only be modified by Aircraft Data Stewards</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Aircraft_Name_Unique</fullName>
        <active>true</active>
        <description>Aircraft Name must be unique</description>
        <errorConditionFormula>and(
    Name = VLOOKUP
       ($ObjectType.L2I_Aircraft__c.Fields.Name , $ObjectType.L2I_Aircraft__c.Fields.Name, Name ),

    not(
     Id=VLOOKUP
         ($ObjectType.L2I_Aircraft__c.Fields.Id , $ObjectType.L2I_Aircraft__c.Fields.Name, Name )))</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>The name of your Aircraft must be unique</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Block_Number_Format</fullName>
        <active>true</active>
        <description>Block Number cannot start with &quot;MSN&quot;</description>
        <errorConditionFormula>BEGINS( L2I_Block_Number__c  , &quot;MSN&quot;)</errorConditionFormula>
        <errorDisplayField>L2I_Block_Number__c</errorDisplayField>
        <errorMessage>Invalid Block Number Entered</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Fleet_Number_Format</fullName>
        <active>true</active>
        <description>Fleet Number cannot start with &quot;MSN&quot;</description>
        <errorConditionFormula>BEGINS(L2I_Fleet_Number__c , &quot;MSN&quot;)</errorConditionFormula>
        <errorDisplayField>L2I_Fleet_Number__c</errorDisplayField>
        <errorMessage>Invalid Fleet Number Entered</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_GPIN_Charcter_Limits</fullName>
        <active>false</active>
        <description>GPIN must only contain the following characters: A-Z, a-z, 0-9 and dash &quot;-&quot;. This only applies to users with a profile/permission set containing &quot;L2I_Aircraft_Validation&quot; custom permission</description>
        <errorConditionFormula>AND(
NOT(REGEX( L2I_GPIN__c,&quot;^[a-zA-Z0-9\\-]*$&quot;)),
$Permission.L2I_Aircraft_Validation)</errorConditionFormula>
        <errorDisplayField>L2I_GPIN__c</errorDisplayField>
        <errorMessage>Please enter a valid GPIN (characters allowed A-Z, a-z, 0-9 and dash &quot;-&quot;)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_GPIN_Format</fullName>
        <active>true</active>
        <description>Using only select characters (a-z, A-Z, 0-9), GPIN must contain 3 or 4 characters prior to the dash and then 27 characters after. This only applies to users with a profile/permission set containing &quot;L2I_Aircraft_Validation&quot; custom permission</description>
        <errorConditionFormula>AND(
NOT(REGEX( L2I_GPIN__c,&quot;^[a-zA-Z0-9]{3,4}-[a-zA-Z0-9]*$&quot;)),
 $Permission.L2I_Aircraft_Validation)</errorConditionFormula>
        <errorDisplayField>L2I_GPIN__c</errorDisplayField>
        <errorMessage>Please enter a valid aircraft type designator for this GPIN</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Line_Number_Format</fullName>
        <active>true</active>
        <description>Line Number cannot start with &quot;MSN&quot;</description>
        <errorConditionFormula>BEGINS(L2I_Line_Number__c , &quot;MSN&quot;)</errorConditionFormula>
        <errorDisplayField>L2I_Line_Number__c</errorDisplayField>
        <errorMessage>Invalid Line Number Entered</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Preliminary_GPIN_Format</fullName>
        <active>true</active>
        <description>Preliminary GPIN must only contain the following characters: A-Z, a-z, 0-9 and dash &quot;-&quot;. This only applies to users with a profile/permission set containing &quot;L2I_Aircraft_Validation&quot; custom permission</description>
        <errorConditionFormula>AND(
NOT(REGEX( L2I_Preliminary_GPIN__c ,&quot;^[a-zA-Z0-9\\-]*$&quot;)),
 $Permission.L2I_Aircraft_Validation)</errorConditionFormula>
        <errorDisplayField>L2I_Preliminary_GPIN__c</errorDisplayField>
        <errorMessage>Please enter a valid Preliminary GPIN (characters allowed A-Z, a-z, 0-9 and dash &quot;-&quot;)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Registration_Number_Format</fullName>
        <active>true</active>
        <description>Registration cannot start with &quot;MSN&quot;</description>
        <errorConditionFormula>BEGINS( L2I_Registration_Number__c , &quot;MSN&quot;)</errorConditionFormula>
        <errorDisplayField>L2I_Registration_Number__c</errorDisplayField>
        <errorMessage>Invalid Registration Number Entered</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Registration_Number_Unique</fullName>
        <active>true</active>
        <description>Registration Number must be different than Block, Fleet, Line Number</description>
        <errorConditionFormula>AND( 
NOT(ISBLANK( L2I_Registration_Number__c)), 
AND(OR(
LOWER( L2I_Registration_Number__c ) = LOWER( L2I_Block_Number__c ),
LOWER( L2I_Registration_Number__c ) = LOWER( L2I_Fleet_Number__c ),
LOWER( L2I_Registration_Number__c ) = LOWER( L2I_Line_Number__c ),
LOWER( L2I_Registration_Number__c ) = LOWER( L2I_Serial_Number__c )
)))</errorConditionFormula>
        <errorDisplayField>L2I_Registration_Number__c</errorDisplayField>
        <errorMessage>Please enter a valid aircraft registration number assigned by a civil aviation authority. The Block, Fleet, Line and Serial numbers must be unique</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Serial_Number_Format</fullName>
        <active>true</active>
        <description>Serial Number cannot start with &quot;MSN&quot;</description>
        <errorConditionFormula>BEGINS( L2I_Serial_Number__c  , &quot;MSN&quot;)</errorConditionFormula>
        <errorDisplayField>L2I_Serial_Number__c</errorDisplayField>
        <errorMessage>Invalid Serial Number Entered</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Serial_Number_Matches_Block</fullName>
        <active>true</active>
        <description>Serial Number must be different than Block Number</description>
        <errorConditionFormula>AND( 
NOT(ISBLANK( L2I_Serial_Number__c )), 
NOT(ISBLANK ( L2I_Block_Number__c )), 
CONTAINS(LOWER(  L2I_Serial_Number__c  ), LOWER( L2I_Block_Number__c )))</errorConditionFormula>
        <errorDisplayField>L2I_Serial_Number__c</errorDisplayField>
        <errorMessage>Please enter a valid Serial Number</errorMessage>
    </validationRules>
