    <validationRules>
        <fullName>Aircraft_Confirmation_Check</fullName>
        <active>true</active>
        <description>This is to lock the Aircraft Confirmation once it&#39;s been checked</description>
        <errorConditionFormula>AND ( 
    AND($Profile.Name &lt;&gt; &#39;System Administrator&#39;),
    NOT(ISNEW()), 
    ISCHANGED(L2I_Aircraft_Confirmation__c), 
    PRIORVALUE(L2I_Aircraft_Confirmation__c) = true,
				Original_Opportunity__r.SBQQ__PrimaryQuote__r.SBQQ__Ordered__c = true
)</errorConditionFormula>
        <errorMessage>The Aircraft and/or Aircraft Confirmation cannot be changed once it is confirmed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Update_Shipset_Inactive_Rec</fullName>
        <active>true</active>
        <errorConditionFormula>($Profile.Name &lt;&gt; &#39;System Administrator&#39;) &amp;&amp;
RecordType.DeveloperName = &quot;Inactive&quot; &amp;&amp;
(ISCHANGED(Head_End_Date__c) || ISCHANGED(Seat_End_Date__c) || ISCHANGED(Induction_Date__c) || ISCHANGED(Delivery_Date__c) || ISCHANGED(Forecasted_Delivery_Date__c) || ISCHANGED(Aircraft__c))</errorConditionFormula>
        <errorMessage>You are not allowed to make changes to Inactive Shipset.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Delivery_Date</fullName>
        <active>true</active>
        <description>Delivery Date is required for Hardware Delivery Schedules.</description>
        <errorConditionFormula>AND
(
 ISBLANK( Delivery_Date__c ),
 TEXT(Delivery_Schedule_Header__r.Product_Category__c) == &#39;IFEC&#39;,
 AND($Profile.Name &lt;&gt; &#39;System Administrator&#39;) 
)</errorConditionFormula>
        <errorDisplayField>Delivery_Date__c</errorDisplayField>
        <errorMessage>Delivery Date is required for Hardware Delivery Schedules.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Estimated_Start_Dates</fullName>
        <active>false</active>
        <errorConditionFormula>AND
(
 ISBLANK( Delivery_Date__c ),
 ISBLANK( L2I_Advertising_Est_Start_Date__c ),
 ISBLANK( L2I_Connectivity_Est_Start_Date__c ),  
 ISBLANK( L2I_Games_Est_Start_Date__c ), 
 ISBLANK( L2I_TV_Est_Start_Date__c )
)</errorConditionFormula>
        <errorMessage>For Hardware Delivery Schedules you must fill out the Delivery Date.
For Services Delivery Schedules you must fill out at least one of the below Estimated Start of Service Dates.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Linked_DSL_Aircraft</fullName>
        <active>true</active>
        <description>If the DSL is linked to an IFEC DSL then the Aircraft cannot be changed</description>
        <errorConditionFormula>AND(NOT(ISBLANK(IFEC_DSL__c)),  ISCHANGED( Aircraft__c ),AND($Profile.Name &lt;&gt; &#39;System Administrator&#39;)  )</errorConditionFormula>
        <errorMessage>Aircraft Identifier may not be changed when DSL is linked to an IFEC DSL.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Validate_Aircraft</fullName>
        <active>true</active>
        <errorConditionFormula>AND(L2I_Aircraft_Confirmation__c = TRUE,
AND($Profile.Name &lt;&gt; &#39;System Administrator&#39;),
OR (

AND((NOT(ISBLANK(IFEC_DSL__c))),
   OR(NOT(IFEC_DSL__r.Aircraft__r.L2I_Approved__c), ISBLANK(IFEC_DSL__r.Aircraft__c) )),

AND((ISBLANK(IFEC_DSL__c)),
OR(NOT(Aircraft__r.L2I_Approved__c), ISBLANK(Aircraft__c) ))
)
)</errorConditionFormula>
        <errorMessage>Selection of an Approved Aircraft is needed with Aircraft Confirmation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Original_Opportunity_Required</fullName>
        <active>false</active>
        <description>The Original Opportunity field is required when creating or editing a DSL. This rule can be turned off via a custom setting value.</description>
        <errorConditionFormula>AND ($Setup.L2I_Rule_Settings__c.Enforce_Validation_Rule__c,
					 ISBLANK(Original_Opportunity__c) )</errorConditionFormula>
        <errorDisplayField>Original_Opportunity__c</errorDisplayField>
        <errorMessage>An Original Opportunity value is required on a DSL.</errorMessage>
    </validationRules>
