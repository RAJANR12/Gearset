        <fullName>PT_Aircraft_Program_Identification_Field__c</fullName>
        <description>The Program specific Identifier of the Aircraft</description>
        <externalId>false</externalId>
        <formula>if( Text(PT_Program__r.Aircraft_Identifier_Field__c) = &#39;Block Number&#39;, Aircraft__r.L2I_Block_Number__c
,IF(Text(PT_Program__r.Aircraft_Identifier_Field__c) = &#39;Fleet Number&#39;, Aircraft__r.L2I_Fleet_Number__c
,IF(Text(PT_Program__r.Aircraft_Identifier_Field__c) = &#39;Line Number&#39;, Aircraft__r.L2I_Line_Number__c
,IF(Text(PT_Program__r.Aircraft_Identifier_Field__c) = &#39;Serial Number&#39;, Aircraft__r.L2I_Serial_Number__c
,IF(Text(PT_Program__r.Aircraft_Identifier_Field__c) = &#39;Tail Number&#39;, Aircraft__r.L2I_Registration_Number__c
, Aircraft_Identifier__c
)))))</formula>
