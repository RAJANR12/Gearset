    <validationRules>
        <fullName>Cannot_Change_FitType_On_Delivery_Stage</fullName>
        <active>true</active>
        <description>When the Opportunity Stage is Delivery then Fit Type cannot be changed</description>
        <errorConditionFormula>If(IsChanged( L2I_Fit_Type__c )&amp;&amp; IsPICKVAL(StageName, &quot;Delivery&quot;) &amp;&amp; ($Profile.Name &lt;&gt; &#39;System Administrator&#39;), TRUE, FALSE)</errorConditionFormula>
        <errorDisplayField>L2I_Fit_Type__c</errorDisplayField>
        <errorMessage>You cannot change the Fit Type when the stage is Delivery</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Update_Inactive_PPL</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISCHANGED(PT_Status__c)) &amp;&amp; TEXT(PT_Status__c) = &#39;Inactive&#39; &amp;&amp;  RecordType.DeveloperName = &#39;PPL&#39; &amp;&amp; ($Profile.Name &lt;&gt; &#39;System Administrator&#39;)</errorConditionFormula>
        <errorMessage>You are not allowed to make changes to Inactive Non-Shipset (PPL).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DSS_Cannot_have_Program_Number</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL(L2I_Product_Category__c, &#39;DSS&#39;)
,NOT(ISBLANK(L2I_Program_Num__c))
,AND($Profile.Name &lt;&gt; &#39;System Administrator&#39;) )</errorConditionFormula>
        <errorMessage>A Product Categoy of DSS cannot have a Program Number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Deal_NDA_Validation</fullName>
        <active>true</active>
        <description>When is Customer NDA Valid is Other, NDA Details are required</description>
        <errorConditionFormula>AND(
    RecordType.DeveloperName = &quot;Deal&quot;, 
    ISPICKVAL(L2I_NDA_OnFile__c, &quot;Other&quot;),
  		ISBLANK(L2I_NDA_Details__c),
$Profile.Name &lt;&gt; &#39;System Administrator&#39;
)</errorConditionFormula>
        <errorDisplayField>L2I_NDA_Details__c</errorDisplayField>
        <errorMessage>NDA Details are required when Is Customer NDA Valid is selected as Other.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Decision_Drivers_Required</fullName>
        <active>true</active>
        <description>At least one Decision Driver is required before setting the Sales Opportunity Stage to Closed Lost or Closed Won.</description>
        <errorConditionFormula>And(
OR(
RecordType.DeveloperName = &quot;Sales&quot;,
RecordType.DeveloperName = &quot;Amendment&quot;,
RecordType.DeveloperName = &quot;Renewal&quot;),
$Profile.Name &lt;&gt; &#39;System Administrator&#39;,
ISBLANK(Decision_Drivers__c),

OR(
Ispickval(StageName, &quot;Closed Lost&quot;),
Ispickval(StageName, &quot;Closed Won&quot;)

))</errorConditionFormula>
        <errorMessage>You must select at least one Decision Driver before setting the Sales Opportunity Stage to Closed Lost or Closed Won.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DoNotAllow_NewSales_with_Stages</fullName>
        <active>true</active>
        <description>Do not allow users to create New Sales Opportunities with Stages after Negotiation.</description>
        <errorConditionFormula>AND(

OR(
isNew_From_QuickAction__c,
ISNEW()),

RecordType.Name == &quot;Sales&quot;,

OR(
ISPICKVAL(StageName, &quot;Negotiation&quot;),
ISPICKVAL(StageName, &quot;Procurement&quot;),
ISPICKVAL(StageName, &quot;Pending Close&quot;)
)

,AND($Profile.Name &lt;&gt; &#39;System Administrator&#39;)
)</errorConditionFormula>
        <errorMessage>A Sales Opportunity must be created in Prospecting, Qualification or Quotation stages only.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DoNot_Allow_Stages_On_NewSales_Opp</fullName>
        <active>true</active>
        <description>On new sales opportunity do not allow Closed won, Closed lost, Handoff, Delivery</description>
        <errorConditionFormula>If(  isNew_From_QuickAction__c &amp;&amp; ISNEW() &amp;&amp;  RecordType.Name ==&quot;Sales&quot; &amp;&amp;  (ISPICKVAL(StageName,&quot;Closed Won&quot;) || IsPICKVAL(StageName, &quot;Closed lost&quot; ) || ISPICKVAL(StageName ,&quot;handoff&quot; ) || ISPICKVAL(StageName, &quot;Delivery&quot;)) &amp;&amp;($Profile.Name &lt;&gt; &#39;System Administrator&#39;)   , TRUE, FALSE)</errorConditionFormula>
        <errorMessage>On  a new sales opportunity   the stage cannot be Closed won, Closed lost, Handoff or Delivery</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FitType_Not_Be_Changed_Program_Inactive</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(L2I_Program_Num__r.PT_isActive__c, &quot;Inactive&quot; ) &amp;&amp;  ISCHANGED( L2I_Fit_Type__c) &amp;&amp; ($Profile.Name &lt;&gt; &#39;System Administrator&#39;)</errorConditionFormula>
        <errorMessage>When Program is Inactive, Fit Type on Opportunity cannot be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IFEC_Product_Category_Required_Fields</fullName>
        <active>false</active>
        <description>If a Sales Opportunity has Product Category = IFEC then require:
Are These Follow on Aircraft?
Are These Partial Shipsets?
First Class Cabin Configuration
Business Class Cabin Configuration
Premium Economy Cabin Configuration</description>
        <errorConditionFormula>AND(
RecordType.DeveloperName = &quot;Sales&quot;, 
ISPICKVAL(L2I_Product_Category__c, &quot;IFEC&quot;),

OR(
ISBLANK(TEXT(L2I_Are_These_Follow_On_Aircraft__c)),
ISBLANK(TEXT(L2I_Are_These_Partial_Shipsets__c)),
ISBLANK(TEXT(L2I_First_Class_Cabin_Configuration__c)),
ISBLANK(TEXT(L2I_Business_Class_Cabin_Configuration__c)),
ISBLANK(TEXT(L2I_Premium_Economy_Cabin_Configuration__c)),
ISBLANK(TEXT(L2I_Economy_Cabin_Configuration__c))
))</errorConditionFormula>
        <errorMessage>For Product Type = IFEC, the following fields are required:
Are These Follow on Aircraft?
Are These Partial Shipsets?
First Class Cabin Configuration
Business Class Cabin Configuration
Premium Economy Cabin Configuration</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_AdvertisingClosedWon_Required_Fields</fullName>
        <active>true</active>
        <errorConditionFormula>And(

RecordType.DeveloperName = &quot;L2I_Advertising&quot;,
ISPICKVAL(StageName,&quot;Closed Won&quot;),
$Profile.Name &lt;&gt; &#39;System Administrator&#39;,

AND(
OR(
ISBLANK(L2I_Advertiser_Name__c),
ISBLANK(Name),
ISBLANK(L2I_Campaign_Start_Date__c),
ISBLANK(L2I_Campaign_End_Date__c),
ISPICKVAL(L2I_Pricing_Model__c, &quot;&quot;)

)

)

)</errorConditionFormula>
        <errorMessage>You must fill in the values for the following field before setting to Closed/Won stage.
Advertiser Name
Opportunity Name
Start Date
End Date
Pricing Model</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_ClosedLost_Reasoning_Required</fullName>
        <active>true</active>
        <errorConditionFormula>$Setup.L2I_Rule_Settings__c.Enforce_Validation_Rule__c &amp;&amp;
And(
OR(
RecordType.DeveloperName = &quot;Sales&quot;,
RecordType.DeveloperName = &quot;Amendment&quot;,
RecordType.DeveloperName = &quot;Renewal&quot;),
ISBLANK(L2I_Closed_Lost_Reasoning__c),

OR( 
Ispickval(StageName, &quot;Closed Lost&quot;),
Ispickval(StageName, &quot;Cancelled&quot;),
Ispickval(StageName, &quot;No Bid&quot;)
)
,AND($Profile.Name &lt;&gt; &#39;System Administrator&#39;) 
)</errorConditionFormula>
        <errorMessage>You must fill out a Closed NOT WON Reasoning before you can set a Sales Opportunities Stage to Closed Lost, Cancelled or No Bid</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Customer_Sig_Date_Validation</fullName>
        <active>true</active>
        <description>This restricts users from creating the Customer Signature Date field who aren&#39;t in the &quot;Contract Manager&quot; Permission Set.</description>
        <errorConditionFormula>AND( 
L2I_Customer_Signature_Set__c = True, 
$Permission.L2I_Fire_Contract_Super_User_Process != True, 
ISCHANGED(L2I_Customer_Signature_Date__c), AND($Profile.Name &lt;&gt; &#39;System Administrator&#39;) 
)</errorConditionFormula>
        <errorDisplayField>L2I_Customer_Signature_Date__c</errorDisplayField>
        <errorMessage>You must be a Contract Manager Super User to update this field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_DeliveryCAP_Validation</fullName>
        <active>true</active>
        <description>only one of the 2 fields can be populated by the user.</description>
        <errorConditionFormula>L2I_Delivery_SLA_Penalty_Cap_Percent__c != 0  &amp;&amp;   L2I_Delivery_SLA_Penalty_Cap_Amount__c !=0 &amp;&amp; ($Profile.Name &lt;&gt; &#39;System Administrator&#39;)</errorConditionFormula>
        <errorMessage>Delivery/Cap can only be a Percent % or Dollar $</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Delivery_Sched_cannot_be_changed</fullName>
        <active>false</active>
        <description>The Delivery Schedule may not be changed.</description>
        <errorConditionFormula>$Setup.L2I_Rule_Settings__c.Enforce_Validation_Rule__c &amp;&amp;
AND(NOT(ISBLANK(PRIORVALUE(L2I_Delivery_Schedule__c))),  
				L2I_Delivery_Schedule__c &lt;&gt; PRIORVALUE(L2I_Delivery_Schedule__c),
				NOT(ISNEW())
			)</errorConditionFormula>
        <errorDisplayField>L2I_Delivery_Schedule__c</errorDisplayField>
        <errorMessage>The Delivery Schedule may not be changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Enforce_No_Parent</fullName>
        <active>true</active>
        <errorConditionFormula>$Setup.L2I_Opportunity_Setting__c.Enforce_No_Parent__c &amp;&amp;
!ISBLANK( L2I_Parent_Opportunity__c ) &amp;&amp;
(
  RecordType.DeveloperName == &#39;Deal&#39; ||
  RecordType.DeveloperName == &#39;Market&#39; ||
  RecordType.DeveloperName == &#39;Advertising&#39; 
) &amp;&amp;($Profile.Name &lt;&gt; &#39;System Administrator&#39;)</errorConditionFormula>
        <errorMessage>Opportunities of type Deal, Market, and Advertising cannot have a parent Opportunity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Frequency_Capping_Number</fullName>
        <active>true</active>
        <description>If Frequency Capping yes is selected, validation value for this field a single digit numerical value 1-9</description>
        <errorConditionFormula>ISBLANK( L2I_Frequency_Cap__c)  &amp;&amp;  (L2I_Frequency_Capping__c  = True) &amp;&amp; ($Profile.Name &lt;&gt; &#39;System Administrator&#39;)</errorConditionFormula>
        <errorDisplayField>L2I_Frequency_Cap__c</errorDisplayField>
        <errorMessage>You must provide a number, 1-9, for frequency capping</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Fulfillment_Requires_Pipeline</fullName>
        <active>false</active>
        <errorConditionFormula>$Setup.L2I_Opportunity_Setting__c.Enforce_Fulfillment_Parent__c &amp;&amp;
RecordType.DeveloperName == &#39;Fulfillment&#39; &amp;&amp;
( 
  ISBLANK( L2I_Parent_Opportunity__c ) ||
   L2I_Parent_Opportunity__r.RecordType.DeveloperName != &#39;Sales&#39;
)</errorConditionFormula>
        <errorMessage>Fulfillment Opportunities require a Sales Opportunity as a parent.  Please select the appropriate Sales Opportunity in the Parent Opportunity field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Missing_Competitor_Association</fullName>
        <active>true</active>
        <errorConditionFormula>And( $Profile.Name != &quot;System Administrator&quot;,
    OR(
        RecordType.DeveloperName = &quot;Sales&quot;,
        RecordType.DeveloperName = &quot;Amendment&quot;,
        RecordType.DeveloperName = &quot;Renewal&quot; &amp;&amp; NOT(L2I_AutoExtend__c)
				),
    L2I_Competitor_Count__c = 0,
    OR(
        Ispickval(StageName, &quot;Pending Close&quot;),
        Ispickval(StageName, &quot;Closed Won&quot;),
        Ispickval(StageName, &quot;Closed&quot;),				
        Ispickval(StageName, &quot;No Bid&quot;),
        Ispickval(StageName, &quot;Closed Lost&quot;)
    ),
AND($Profile.Name &lt;&gt; &#39;System Administrator&#39;)
)</errorConditionFormula>
        <errorMessage>Sales, Amendment and Renewal Opportunities must have a Primary Competitor defined before they can be put into Stages Pending Close, Closed, Closed Won, Closed Lost or No Bid</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Negotiation_Required_Fields</fullName>
        <active>true</active>
        <errorConditionFormula>AND(

NOT(ISNEW()), 
RecordType.Name = &quot;Sales&quot;,

OR(
ISPICKVAL(StageName, &quot;Negotiation&quot;),
ISPICKVAL(StageName, &quot;Procurement&quot;),
ISPICKVAL(StageName, &quot;Pending Close&quot;),
ISPICKVAL(StageName, &quot;Closed Won&quot;)),

AND(
  ISBLANK(L2I_Initial_Proposal_Sub_Date_from_Quote__c)  
),
AND($Profile.Name &lt;&gt; &#39;System Administrator&#39;)
)</errorConditionFormula>
        <errorMessage>When the Sales Opportunity Stage is Negotiation, Procurement, Pending Close, or Closed Won, you must populate the Initial Proposal Submission Date field on the Primary Quote.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_No_Edit_Amend_Renew_Prod_Category</fullName>
        <active>false</active>
        <description>Product Category may not be changed for Amendments and Renewals.</description>
        <errorConditionFormula>AND(
 OR(RecordType.DeveloperName = &#39;Amendment&#39;, RecordType.DeveloperName = &#39;Renewal&#39;),
TEXT(PRIORVALUE(L2I_Product_Category__c)) &lt;&gt; TEXT(L2I_Product_Category__c) )</errorConditionFormula>
        <errorDisplayField>L2I_Product_Category__c</errorDisplayField>
        <errorMessage>Product Category may not be changed for Amendments and Renewals.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_PendingClose_Required_Fields</fullName>
        <active>true</active>
        <errorConditionFormula>AND(

NOT(ISNEW()), 
RecordType.Name = &quot;Sales&quot;,

OR(
ISPICKVAL(StageName, &quot;Pending Close&quot;),
ISPICKVAL(StageName, &quot;Closed Won&quot;)),

AND(
    ISBLANK(L2I_Final_Proposal_Sub_Date_from_Quote__c)  

),
AND($Profile.Name &lt;&gt; &#39;System Administrator&#39;)
)</errorConditionFormula>
        <errorMessage>When the Sales Opportunity Stage is Pending Close or Closed Won, you must populate the Final Proposal Submission Date field on the Primary Quote.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Pipeline_Requires_Deal</fullName>
        <active>true</active>
        <description>Validate that Deal Opportunity is selected only as Parent on Sales Opportunity upon change and Create</description>
        <errorConditionFormula>AND(
OR(
RecordType.DeveloperName = &quot;Sales&quot;,
RecordType.DeveloperName = &quot;Amendment&quot;,
RecordType.DeveloperName = &quot;Renewal&quot;),
OR( 
ISBLANK( L2I_Parent_Opportunity__c ),
L2I_Parent_Opportunity__r.RecordType.DeveloperName != &#39;Deal&#39;
),
AND($Profile.Name &lt;&gt; &#39;System Administrator&#39;))</errorConditionFormula>
        <errorMessage>Sales Opportunities require a Deal Opportunity as a parent.  Please select the appropriate Deal Opportunity in the Parent Opportunity field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Prevent_Stage_Change_for_Obs_Quote</fullName>
        <active>true</active>
        <description>Opportunity may not be changed from Closed Lost when the Primary Quote is Obsolete.</description>
        <errorConditionFormula>AND( 
				OR( 
								And (Text(StageName) &lt;&gt; &#39;Closed Lost&#39;, 
													Text(PRIORVALUE(StageName)) = &#39;Closed Lost&#39;),
								And (Text(StageName) &lt;&gt; &#39;Cancelled&#39;, 
													Text(PRIORVALUE(StageName)) = &#39;Cancelled&#39;),
								And (Text(StageName) &lt;&gt; &#39;No Bid&#39;, 
													Text(PRIORVALUE(StageName)) = &#39;No Bid&#39;)
				),
				SBQQ__PrimaryQuote__r.L2I_Obsolete__c = TRUE,
$Profile.Name &lt;&gt; &#39;System Administrator&#39;
)</errorConditionFormula>
        <errorMessage>Opportunity may not be changed from Closed Lost, Cancelled, No Bid when the Primary Quote is Obsolete.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Product_Category_Validation</fullName>
        <active>true</active>
        <description>The Product Category on an Opportunity with a Delivery Schedule may not changed.</description>
        <errorConditionFormula>AND(
 OR(RecordType.DeveloperName = &#39;Sales&#39;, RecordType.DeveloperName = &#39;Amendment&#39;, RecordType.DeveloperName = &#39;Renewal&#39;),
 Not(IsBlank( L2I_Delivery_Schedule__c )),
 ISCHANGED(L2I_Product_Category__c ),
AND($Profile.Name &lt;&gt; &#39;System Administrator&#39;))</errorConditionFormula>
        <errorDisplayField>L2I_Product_Category__c</errorDisplayField>
        <errorMessage>The Product Category on an Opportunity with a Delivery Schedule may not changed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Validate_Approved_Quote</fullName>
        <active>true</active>
        <description>Primary Quote must be Approved when changing to Opportunity to Closed Won/Handoff/Delivery</description>
        <errorConditionFormula>NOT(ISPICKVAL(SBQQ__PrimaryQuote__r.ApprovalStatus__c, &#39;Approved&#39;)) &amp;&amp;
ISCHANGED(StageName) &amp;&amp;
(Ispickval(StageName, &quot;Closed Won&quot;) || Ispickval(StageName, &quot;Handoff&quot;) || Ispickval(StageName, &quot;Delivery&quot;)) &amp;&amp;
(RecordType.DeveloperName = &quot;PPL&quot; || ((RecordType.DeveloperName = &quot;Sales&quot; || RecordType.DeveloperName = &quot;Amendment&quot; || RecordType.DeveloperName = &quot;Renewal&quot;) &amp;&amp; ISPICKVAL(L2I_Product_Category__c, &quot;DSS&quot;)) || (RecordType.DeveloperName = &quot;Sales&quot; &amp;&amp; (ISPICKVAL(L2I_Product_Category__c, &quot;IFEC&quot;) || ISPICKVAL(L2I_Product_Category__c, &quot;PTS&quot;))))
&amp;&amp;($Profile.Name &lt;&gt; &#39;System Administrator&#39;)</errorConditionFormula>
        <errorMessage>Primary Quote must be Approved when changing the Opportunity to Closed Won or Handoff or Delivery</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Validate_Delivery_Schedule</fullName>
        <active>true</active>
        <errorConditionFormula>And(

RecordType.DeveloperName = &quot;Sales&quot;,
ISBLANK(L2I_Delivery_Schedule__c), 
OR(
Ispickval(StageName, &quot;Finalization&quot;),
Ispickval(StageName, &quot;Pending Close&quot;),
Ispickval(StageName, &quot;Closed Won&quot;)

)
)
&amp;&amp; $Profile.Name &lt;&gt; &#39;System Administrator&#39;</errorConditionFormula>
        <errorMessage>Sales Opportunity must have a Delivery Schedule defined before they can be put into Stage Closed Won.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Validate_Primary_Quote</fullName>
        <active>false</active>
        <description>Opportunity can not be Pending Close or Closed Won without Primary quote under it.</description>
        <errorConditionFormula>And(
    RecordType.DeveloperName = &quot;Sales&quot;,
    ISBLANK(SBQQ__PrimaryQuote__c),  
    (Ispickval(StageName, &quot;Closed Won&quot;)  
	        || Ispickval(StageName, &quot;Pending Close&quot;)
									|| Ispickval(StageName, &quot;Handoff&quot;)
									|| Ispickval(StageName, &quot;Delivery&quot;) 
    )
    )</errorConditionFormula>
        <errorMessage>Please create one primary quote for this Opportunity to advance to Pending close or Closed Won or Handoff or Delivery Stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Warranty_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
    OR(NOT(ISBLANK(TEXT(L2I_Warranty__c))), 
			    NOT(ISBLANK(TEXT(L2I_Warranty_Start_Date__c))), 
			    NOT(ISBLANK(TEXT(L2I_Standard_in_Warranty_TAT__c)))
			   ),
 				NOT(L2I_Allow_Warranty_Fields__c)
				) &amp;&amp; $Profile.Name &lt;&gt; &#39;System Administrator&#39;</errorConditionFormula>
        <errorMessage>Warranty fields only Apply to IFEC Opportunities.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>L2I_Win_Loss_Date_Required</fullName>
        <active>true</active>
        <description>You must fill out a Win Loss Date  before you can set a Sales Opportunities Stage to Closed won, Closed lost, and no bid</description>
        <errorConditionFormula>$Setup.L2I_Rule_Settings__c.Enforce_Validation_Rule__c &amp;&amp;
AND( 
  OR(
    RecordType.DeveloperName = &quot;Sales&quot;,
    RecordType.DeveloperName = &quot;Amendment&quot;,
    RecordType.DeveloperName = &quot;Renewal&quot; &amp;&amp; NOT(L2I_AutoExtend__c)
		),
  ISBLANK(L2I_Win_Loss_Date__c),
  OR(
    ISPICKVAL( StageName , &quot;Closed Won&quot;),
	   ISPICKVAL( StageName , &quot;Closed&quot;),				
    ISPICKVAL( StageName , &quot;Closed Lost&quot;),
    ISPICKVAL( StageName , &quot;No Bid&quot;) 
  ) 
) &amp;&amp; $Profile.Name &lt;&gt; &#39;System Administrator&#39;</errorConditionFormula>
        <errorMessage>You must fill out a Win/Loss Date before you can set a Sales Opportunities Stage to Closed, Closed Won,Closed Lost or No Bid.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Leased_AC_Qty_less_or_equal_to_AC_Qty</fullName>
        <active>false</active>
        <errorConditionFormula>L2I_Leased_Aircraft_Quantity__c &gt;=  L2I_AircraftQuantity__c &amp;&amp; $Profile.Name &lt;&gt; &#39;System Administrator&#39;</errorConditionFormula>
        <errorDisplayField>L2I_Leased_Aircraft_Quantity__c</errorDisplayField>
        <errorMessage>Leased Aircraft Quantity value has to be equal or less than Aircraft Quantity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Obsolescence_required</fullName>
        <active>true</active>
        <errorConditionFormula>And( $Profile.Name != &quot;System Administrator&quot;,
 Probability &gt;=50, 
 ISNULL(TEXT(L2I_Obsolescence_Provisions_Included__c)) 	 
) &amp;&amp; $Profile.Name &lt;&gt; &#39;System Administrator&#39;</errorConditionFormula>
        <errorDisplayField>L2I_Obsolescence_Provisions_Included__c</errorDisplayField>
        <errorMessage>Obsolescence Provisions Included field value is required for Sales Opportunity when Stage is Negotiations or higher.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_PAC_admin_can_change_Delivery_Stage</fullName>
        <active>true</active>
        <description>Once opportunity is marked as Delivery stage, only PAC Admins/PMO Admins can change the stage.</description>
        <errorConditionFormula>TEXT(PRIORVALUE(StageName)) = &quot;Delivery&quot; &amp;&amp; TEXT(StageName)!= &quot;Delivery&quot;
 &amp;&amp;
($Profile.Name != &quot;System Administrator&quot; &amp;&amp; $Profile.Name != &quot;PAC System Administrator&quot; &amp;&amp; NOT($Permission.PAC_PMO_Admin)) &amp;&amp; $Profile.Name &lt;&gt; &#39;System Administrator&#39;</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>Change in the stage is not allowed once opportunity is marked as &quot;Delivery&quot;. Please contact PMO admins.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PM_PMO_Group_SubGroup_Reqd_Delivery</fullName>
        <active>true</active>
        <description>When the stage is changed to delivery the Proposed  Project Manager, the Proposed  PMO Group and the Proposed PMO Sub Group fileds must have a value</description>
        <errorConditionFormula>If(isPickVal(StageName, &#39;Handoff&#39;)  &amp;&amp; ( isBlank(  Proposed_Program_Manager__c ) || isBlank( Proposed_PMO_Group__c)  || isBlank(Proposed_PMO_SubGroup__c ) ) &amp;&amp; $RecordType.Name=&quot;Sales&quot; , True, False) &amp;&amp; $Profile.Name &lt;&gt; &#39;System Administrator&#39;</errorConditionFormula>
        <errorMessage>The associated program must have a Program Manager, PMO Group and a PMO Sub Group</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PT_Primary_System_Type_Validation</fullName>
        <active>true</active>
        <description>Fail the sales opportunity record creation unless primary system type exists.</description>
        <errorConditionFormula>RecordType.DeveloperName = &quot;Sales&quot; &amp;&amp;
ISNEW() &amp;&amp; 
ISBLANK(L2I_Primary_System_Type__r.L2I_System_Type_Translation__c) &amp;&amp;
ISPICKVAL(L2I_Product_Category__c, &quot;IFEC&quot;)</errorConditionFormula>
        <errorDisplayField>L2I_Primary_System_Type__c</errorDisplayField>
        <errorMessage>A Sales Opportunity must be created with a Primary System Type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PT_Program_Name_Validation</fullName>
        <active>true</active>
        <description>Fail the opportunity stage change from handoff to delivery unless the data necessary to generate a program name exists.</description>
        <errorConditionFormula>ISPICKVAL(L2I_Product_Category__c, &quot;IFEC&quot;) 
&amp;&amp; RecordType.DeveloperName = &quot;Sales&quot; 
&amp;&amp; ISBLANK(L2I_Program_Num__c) 
&amp;&amp; (
    (TEXT(PRIORVALUE(StageName)) = &quot;Handoff&quot; 
    &amp;&amp; TEXT(StageName) = &quot;Delivery&quot;) 
    || (TEXT(PRIORVALUE(StageName)) = &quot;Closed Won&quot; 
    &amp;&amp; TEXT(StageName) = &quot;Handoff&quot;)
    ) 
&amp;&amp; (
    ISBLANK(L2I_Primary_System_Type__r.L2I_System_Type_Translation__c)
    || ISBLANK(Account.L2I_Operator_Number__c) 
    || ISBLANK(L2I_Aircraft_Type__r.L2I_Program_Code__c)
)</errorConditionFormula>
        <errorMessage>One or more of these values are not set:
Primary System Type - on Opportunity.
Account Operator Code - on Account.
Aircraft type value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_IFEC_Delivery_Stage</fullName>
        <active>true</active>
        <description>[W-004273] Validation Rule to prevent user except for Sys ad to update Stage to Delivery since the Program Table will not go live in R5</description>
        <errorConditionFormula>AND(
 ISCHANGED(StageName),
 ISPICKVAL(StageName, &#39;Delivery&#39;),
 OR(ISPICKVAL(L2I_Product_Category__c , &quot;IFEC&quot;),
 ISPICKVAL(L2I_Product_Category__c , &quot;DSS&quot;)),
 $Profile.Name &lt;&gt; &quot;System Administrator&quot;,
 $Profile.Name != &quot;PAC System Administrator&quot;
)</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>IFEC and DSS Opportunities cannot be changed to Delivery status to prevent the creation of duplicate Programs and Sales Orders. Please upload your DSS Opportunity with the appropriate IFEC Configuration, to the Oracle Program Table via the P&amp;L Uploader.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Primary_Quote_Can_Not_Be_Deleted</fullName>
        <active>true</active>
        <description>Primary quote can not be deleted</description>
        <errorConditionFormula>NOT( ISNEW() ) &amp;&amp; 
ISCHANGED(SBQQ__PrimaryQuote__c) &amp;&amp;
ISBLANK(SBQQ__PrimaryQuote__c) &amp;&amp;
$Profile.Name &lt;&gt; &#39;System Administrator&#39;</errorConditionFormula>
        <errorDisplayField>SBQQ__PrimaryQuote__c</errorDisplayField>
        <errorMessage>Primary quote can not be deleted</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prospect_Account_Stage_Validations</fullName>
        <active>true</active>
        <description>Sales opportunities which are tied to a Prospect Account cannot have a stage of Procurement or Pending Close or Closed Won. The prospect account has to be converted to a customer account in order to proceed in the sales process</description>
        <errorConditionFormula>AND(Account.RecordType.Name = &quot;Prospect&quot;,
OR(
RecordType.DeveloperName = &quot;Sales&quot;,
RecordType.DeveloperName = &quot;Amendment&quot;,
RecordType.DeveloperName = &quot;Renewal&quot;),
OR(ISPICKVAL(StageName,&quot;Procurement&quot;),
ISPICKVAL(StageName,&quot;Pending Close&quot;),
ISPICKVAL(StageName,&quot;Closed Won&quot;)
),
$Profile.Name &lt;&gt; &quot;System Administrator&quot;
)</errorConditionFormula>
        <errorMessage>The Current Sales Opportunity Is On A Prospect Account. Please Convert The Account To Type Customer Via the Convert To Customer Functionality on the Account Record. Once The Convert To Account Is Complete You Will Be Able To Progress The Sales Stages</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_for_CRB_Request_Validation</fullName>
        <active>true</active>
        <description>When the Commitment Review Board (CRB) Required field is checked, the user must populate the Reason for CRB Request field</description>
        <errorConditionFormula>AND(
RecordType.DeveloperName = &quot;Sales&quot;,
$Profile.Name &lt;&gt; &#39;System Administrator&#39;,
L2I_Commitment_Review_Board_CRB_Required__c = True,
ISBLANK(Reason_for_CRB_Request__c)
)</errorConditionFormula>
        <errorMessage>When Commitment Review Board (CRB) Required is checked, you must populate Reason for CRB Request</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Selected_Aircraft_has_program_code</fullName>
        <active>true</active>
        <description>Fail the lookup to aircraft if it does not have a program code</description>
        <errorConditionFormula>AND(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
ISPICKVAL(L2I_Product_Category__c, &#39;IFEC&#39;),
RecordType.DeveloperName = &quot;Sales&quot;, 
NOT(ISBLANK(L2I_Aircraft_Type__c)),
ISBLANK(L2I_Aircraft_Type__r.L2I_Program_Code__c),
ISPICKVAL(StageName,&quot;Delivery&quot;)
)</errorConditionFormula>
        <errorMessage>The selected aircraft must have a valid Program Code. Please contact system admin to add program code on the selected aircraft.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Shipping_Incoterm_required_on_Deal</fullName>
        <active>false</active>
        <errorConditionFormula>And(
$Profile.Name &lt;&gt; &quot;System Administrator&quot;,
RecordType.DeveloperName = &quot;Sales&quot;,
    OR(ISPICKVAL(StageName, &quot;Negotiation&quot;), 
							ISPICKVAL(StageName, &quot;Procurement&quot;), 
							ISPICKVAL(StageName, &quot;Pending Close&quot;), 
							ISPICKVAL(StageName, &quot;Closed Won&quot;)),
    AND(ISBLANK(TEXT(L2I_Parent_Opportunity__r.L2I_Shipping_Incoterm__c))
							)
			)</errorConditionFormula>
        <errorMessage>A Shipping/Incoterm is required on the Deal Opportunity before setting the Sales Opportunity to Negotiation.</errorMessage>
    </validationRules>
</CustomObject>
