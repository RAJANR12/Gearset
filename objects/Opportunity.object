    <fields>
        <fullName>L2I_Short_Lead_Time__c</fullName>
        <externalId>false</externalId>
        <formula>IF( OR ( AND
            ( 
               ISPICKVAL( L2I_Product_Category__c , &#39;IFEC&#39;), ISPICKVAL(L2I_Fit_Type__c,&#39;Retrofit&#39;) 
            ),
         (AND
             (
                ISPICKVAL( L2I_Product_Category__c , &#39;DSS&#39;), ISPICKVAL(L2I_Fit_Type__c, &#39;Renewal&#39;)
              )
          )
       )
, 
   IF(( L2I_First_Delivery_Date__c  -  CloseDate   &lt;= 425), True, False)
,
IF( OR ( AND
            ( 
               ISPICKVAL( L2I_Product_Category__c , &#39;IFEC&#39;), ISPICKVAL(L2I_Fit_Type__c,&#39;Production&#39;) 
            ),
         (AND
             (
                ISPICKVAL( L2I_Product_Category__c , &#39;DSS&#39;), ISPICKVAL(L2I_Fit_Type__c, &#39;Initial Sale&#39;)
              )
          ),
	 (AND
             (
                ISPICKVAL( L2I_Product_Category__c , &#39;DSS&#39;), ISPICKVAL(L2I_Fit_Type__c, &#39;Upsell/Follow-on&#39;)
              )
          )
       )
, 
   IF(( L2I_First_Delivery_Date__c  -  CloseDate   &lt;= 365), True, False)
,
   False
  )
  )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
