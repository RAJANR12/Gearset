    <fields>
        <fullName>L2I_Suggested_Order_Start_Date__c</fullName>
        <externalId>false</externalId>
        <formula>IF(L2I_Media_Frequency__c = &quot;Monthly (12)&quot;, L2I_1st_Recurring_MMA_Freeze_Date__c - 30,
IF(L2I_Media_Frequency__c = &quot;Bi-Monthly (6)&quot;, L2I_1st_Recurring_MMA_Freeze_Date__c - 60,
IF(L2I_Media_Frequency__c = &quot;Quarterly (4)&quot;, L2I_1st_Recurring_MMA_Freeze_Date__c - 90,
IF(L2I_Media_Frequency__c = &quot;Tri-Annually (3)&quot;, L2I_1st_Recurring_MMA_Freeze_Date__c - 120,
IF(L2I_Media_Frequency__c = &quot;Semi-Annual (2)&quot;, L2I_1st_Recurring_MMA_Freeze_Date__c - 180,
IF(L2I_Media_Frequency__c = &quot;Annually (1)&quot;, L2I_1st_Recurring_MMA_Freeze_Date__c - 365,
null
