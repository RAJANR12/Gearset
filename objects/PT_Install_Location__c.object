    <validationRules>
        <fullName>Pending_MROD_is_Past_Due</fullName>
        <active>true</active>
        <errorConditionFormula>OR(AND(OR (Pending_MROD__c &lt;  TODAY(), ISBLANK( Pending_MROD__c ) ),  
			    ISCHANGED(Pending_MROD__c ),
			    Allow_Past_Due_Pending_MROD_Date__c != true ),
AND(OR (Pending_D1_Date__c &lt;  TODAY(), ISBLANK( Pending_D1_Date__c ) ),  
			    ISCHANGED(Pending_D1_Date__c ),
			    Allow_Past_Due_Pending_MROD_Date__c != true )
)</errorConditionFormula>
        <errorMessage>The Pending MROD or D1 is Past Due. Check the &#39;Allow Past Due&#39; box to continue processing of this date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Seat_Count_and_Seat_Model</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(Solution_Group__r.PT_is_Seat_End__c)
&amp;&amp;
(
NOT(ISBLANK(PT_Seat_Count__c)) ||
NOT(ISBLANK(PT_Seat_Model__c))
)</errorConditionFormula>
        <errorMessage>Only Seat End groups should have the values selected for Seat Count and Seat Model.</errorMessage>
    </validationRules>
